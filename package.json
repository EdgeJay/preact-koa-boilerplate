{
  "name": "preact-koa-boilerplate",
  "version": "0.1.0",
  "description": "Preact + Miligram + Koa + Webpack starter project",
  "main": "starter.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint --config ./.eslintrc.js ./server/*",
    "dev:webpack": "./node_modules/.bin/webpack-dev-server --config ./deploy/config/webpack.dev.js --port 3001",
    "dev:app": "./node_modules/.bin/nodemon -L",
    "build:assets": "rm -rf ./public && cp -R ./resources/ ./public/",
    "build:dev": "npm run build:assets && ./node_modules/.bin/webpack --config ./deploy/config/webpack.dev.js",
    "build:staging": "npm run build:assets && ./node_modules/.bin/webpack --config ./deploy/config/webpack.staging.js",
    "build": "npm run build:assets && ./node_modules/.bin/webpack --config ./deploy/config/webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdgeJay/preact-koa-boilerplate.git"
  },
  "author": "Wu Huijie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdgeJay/preact-koa-boilerplate/issues"
  },
  "homepage": "https://github.com/EdgeJay/preact-koa-boilerplate#readme",
  "devDependencies": {
    "chokidar": "^1.6.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.4",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-css-modules": "^2.6.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "co": "^4.6.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "dustjs-linkedin": "^2.7.5",
    "eslint": "^3.19.0",
